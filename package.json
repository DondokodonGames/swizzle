{
  "name": "swizzle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "ai:generate": "tsx src/ai/index.ts",
    "ai:generate:dry": "VITE_AI_DRY_RUN=true tsx src/ai/index.ts",
    "ai:generate:test": "TARGET_GAMES_COUNT=10 VITE_AI_DRY_RUN=true tsx src/ai/index.ts",
    "ai:improved": "tsx src/ai/run-improved.ts",
    "ai:improved:dry": "VITE_AI_DRY_RUN=true tsx src/ai/run-improved.ts",
    "ai:improved:1": "TARGET_GAMES=1 tsx src/ai/run-improved.ts",
    "ai:improved:1:local": "TARGET_GAMES=1 SKIP_UPLOAD=true tsx src/ai/run-improved.ts",
    "ai:improved:10": "TARGET_GAMES=10 tsx src/ai/run-improved.ts",
    "ai:improved:100": "TARGET_GAMES=100 tsx src/ai/run-improved.ts",
    "ai:v2": "tsx src/ai/v2/run.ts",
    "ai:v2:1": "tsx src/ai/v2/run.ts 1",
    "ai:v2:10": "tsx src/ai/v2/run.ts 10",
    "ai:v2:100": "tsx src/ai/v2/run.ts 100",
    "ai:v2:500": "tsx src/ai/v2/run.ts 500",
    "ai:v2:1000": "tsx src/ai/v2/run.ts 1000",
    "ai:v2:dry": "DRY_RUN=true tsx src/ai/v2/run.ts 1",
    "ai:v2:local": "SKIP_UPLOAD=true tsx src/ai/v2/run.ts 1",
    "ai:evolve": "tsx src/ai/v2/runEvolve.ts",
    "ai:evolve:10": "tsx src/ai/v2/runEvolve.ts 10",
    "ai:evolve:100": "tsx src/ai/v2/runEvolve.ts 100",
    "ai:evolve:1000": "tsx src/ai/v2/runEvolve.ts 1000",
    "marketing:start": "tsx src/marketing/cli.ts start",
    "marketing:post": "tsx src/marketing/cli.ts post",
    "marketing:discord": "tsx src/marketing/cli.ts discord",
    "marketing:twitter": "tsx src/marketing/cli.ts twitter",
    "marketing:instagram": "tsx src/marketing/cli.ts instagram",
    "marketing:status": "tsx src/marketing/cli.ts status",
    "marketing:dry": "MARKETING_DRY_RUN=true tsx src/marketing/cli.ts start"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@stripe/stripe-js": "^8.4.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tabler/icons-react": "^3.34.1",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.3",
    "i18next": "^23.16.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "lucide-react": "^0.544.0",
    "node-cron": "^3.0.3",
    "openai": "^6.7.0",
    "pixi.js": "^7.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.1.0",
    "react-router-dom": "^6.28.0",
    "stripe": "^19.3.0",
    "twitter-api-v2": "^1.17.0",
    "use-sync-external-store": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "tsx": "^4.20.6",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
